{
  "name": "node-knife4j-ui",
  "version": "1.1.0",
  "description": "A Node.js wrapper for Knife4j UI to display Swagger/OpenAPI documentation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --cjs-interop",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "keywords": [
    "knife4j",
    "swagger",
    "openapi",
    "api-docs",
    "documentation",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/766187397/node-knife4j-ui.git"
  },
  "files": [
    "dist",
    "static",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "@nestjs/swagger": ">=7.0.0"
  },
  "devDependencies": {
    "@nestjs/swagger": "^11.2.1",
    "@types/node": "^18.0.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  }
}
